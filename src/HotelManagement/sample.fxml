   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Button fx:id="btn_msg" mnemonicParsing="false" onAction="#pressButton" prefHeight="26.0" prefWidth="123.0" text="Press for Message" textFill="RED" GridPane.rowIndex="1" />
      <Button fx:id="exit_btn" mnemonicParsing="false" onAction="#exitButton" prefHeight="25.0" prefWidth="123.0" text="Exit" GridPane.rowIndex="3" />
   </children>
</GridPane>
